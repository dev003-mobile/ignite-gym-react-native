{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*" : ["src/assets/*"],
      "@components/*" : ["src/components/*"],
      "@contexts/*" : ["src/contexts/*"],
      "@dtos/*" : ["src/dtos/*"],
      "@hooks/*" : ["src/hooks/*"],
      "@routes/*" : ["src/routes/*"],
      "@screens/*" : ["src/screens/*"],
      "@services/*" : ["src/services/*"],
      "@storages/*" : ["src/storages/*"],
      "@utils/*" : ["src/utils/*"]
    }
  }
}
